{
  "edit_predictions": {
    "mode": "eager",
    "copilot": {
      "proxy": null,
      "proxy_no_verify": null,
      "enterprise_uri": null
    },
    "enabled_in_text_threads": false
  },
  "terminal": {
    "dock": "right",
    "font_family": "JetBrainsMono Nerd Font",
    "toolbar": {
      "breadcrumbs": false
    },
    "shell": {
      "with_arguments": {
        "program": "/opt/homebrew/bin/fish",
        "args": ["--login", "--interactive"]
      }
    }
  },
  "agent": {
    "inline_assistant_model": {
      "provider": "anthropic",
      "model": "claude-sonnet-4-5-latest"
    },
    "default_profile": "ask",
    "default_model": {
      "provider": "anthropic",
      "model": "claude-sonnet-4-latest"
    },
    "single_file_review": false,
    "play_sound_when_agent_done": true
  },
  "features": {
    "edit_prediction_provider": "copilot"
  },
  "dev": {
    "lsp_document_colors": "inlay"
  },
  "tab_bar": {
    "show_nav_history_buttons": false,
    "show_tab_bar_buttons": false
  },
  "tabs": {
    "git_status": true,
    "activate_on_close": "neighbour",
    "show_close_button": "hidden"
  },
  "git": {
    "inline_blame": {
      "enabled": false
    }
  },
  "toolbar": {
    "code_actions": false,
    "agent_review": false,
    "quick_actions": false,
    "selections_menu": false,
    "breadcrumbs": false
  },
  "diagnostics": {
    "inline": {
      "enabled": true,
      "update_debounce_ms": 150,
      "padding": 4,
      "min_column": 0,
      "max_severity": null
    }
  },
  "project_panel": {
    "auto_reveal_entries": false,
    "auto_fold_dirs": false,
    "default_width": 100,
    "scrollbar": {
      "show": "never"
    }
  },
  "vim_mode": true,
  "ui_font_size": 16,
  "buffer_font_size": 15,
  "confirm_quit": true,
  "theme": {
    "mode": "system",
    "light": "David's Nordic",
    "dark": "David's Nordic"
  }
}
